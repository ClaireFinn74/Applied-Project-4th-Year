<Page
    x:Class="_4thYearAppliedProject.AttentionLevels.Picture_Perect.PicturePerfect6"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:_4thYearAppliedProject.AttentionLevels.Picture_Perect"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button x:Name="btnBeginSmartPhone" Background="Chocolate" Visibility="Collapsed" HorizontalAlignment="Left" Margin="53,565,0,0" VerticalAlignment="Top" Click="btnBeginSmartPhone_Click" IsEnabled="True" Height="74" Width="252" RenderTransformOrigin="0.5,0.5"
         Content="Ready? Press here to begin!"/>
        <Button x:Name="btnPicturePerfect" Background="LightSalmon" Opacity="1" HorizontalAlignment="Left" Margin="47,486,0,0" VerticalAlignment="Top" Click="btnPicturePerfect_Click" IsEnabled="True" Height="74" Width="252" RenderTransformOrigin="0.5,0.5"
         Content="Ready? Press here to see a picture!"/>
        <Image x:Name="imgPicturePerfectSmartPhone" Visibility="Collapsed" Source="/Images/PicturePerfectSmartPhone.jpg" HorizontalAlignment="Left" Width="597" Margin="36,0,-273,212" Height="401" VerticalAlignment="Bottom" RenderTransformOrigin="1.719,-3.484" />
    </Grid>
</Page>
