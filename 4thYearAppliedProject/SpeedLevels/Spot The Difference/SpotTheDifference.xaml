        <!--Image Block for 1st image-->
        <Image x:Name="Spot1" Visibility="Visible" Source="/Images/SpotTheDifference.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,200,0,0"  d:IsLocked="True" Tapped="Spot1_Tapped" />
        <Image x:Name="Spot1Difference" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,200,0,0"  d:IsLocked="True" Tapped="Spot1Difference_Tapped" />
        <Image x:Name="Spot1Difference2" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted2.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,200,0,0"  d:IsLocked="True" Tapped="Spot1Difference2_Tapped" />
        <Image x:Name="Spot1Difference3" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted3.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,200,0,0"  d:IsLocked="True" Tapped="Spot1Difference3_Tapped" />
        <Image x:Name="Spot1Difference4" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted4.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,200,0,0"  d:IsLocked="True" Tapped="Spot1Difference4_Tapped" />
        <Image x:Name="Spot1Difference5" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted5.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,200,0,0"  d:IsLocked="True" Tapped="Spot1Difference5_Tapped" />
        <Image x:Name="Spot1Difference6" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted6.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,200,0,0"  d:IsLocked="True" Tapped="Spot1Difference6_Tapped" />


        <!--Image Block for 2nd image-->
