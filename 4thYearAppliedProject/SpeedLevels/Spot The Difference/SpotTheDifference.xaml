        <TextBox x:Name="txtHighscore" Height="20" Width="20" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,30,25,0" IsReadOnly="True" />

        <TextBlock x:Name="TimerLog" Height ="40" Width="120" FontSize="20" Margin="0,77,200,500"></TextBlock>
        <TextBlock x:Name="TimerStatus" FontSize="15" Height="40" Width="120" Margin="0,77,-200,500" ></TextBlock>

        <Button x:Name="btnInfo" Opacity="1" Content="Info" HorizontalAlignment="Left" Margin="0,10,10,0" VerticalAlignment="Top" Background="Blue" Foreground="White" IsEnabled="True" Height="60" Width="240" RenderTransformOrigin="0.5,0.5" Click="btnInfo_Click"/>

    </Grid>
</Page>