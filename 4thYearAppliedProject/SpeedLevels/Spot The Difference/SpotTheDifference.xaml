        <Image x:Name="Spot3Difference6" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted6.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,350,0,0"  d:IsLocked="True" Tapped="Spot3Difference6_Tapped" />



        <!--Image block for 4th image-->
        <Image x:Name="Spot4" Visibility="Visible" Source="/Images/SpotTheDifference4.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,350,0,0"  d:IsLocked="True" Tapped="Spot4_Tapped" />
        <Image x:Name="Spot4Difference" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,350,0,0"  d:IsLocked="True" Tapped="Spot4Difference_Tapped" />
        <Image x:Name="Spot4Difference2" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted2.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,350,0,0"  d:IsLocked="True" Tapped="Spot4Difference2_Tapped" />
        <Image x:Name="Spot4Difference3" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted3.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,350,0,0"  d:IsLocked="True" Tapped="Spot4Difference3_Tapped"/>
        <Image x:Name="Spot4Difference4" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted4.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,350,0,0"  d:IsLocked="True" Tapped="Spot4Difference4_Tapped"/>
