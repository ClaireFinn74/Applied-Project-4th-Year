        <Image x:Name="Spot2Difference3" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted3.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,200,0,0"  d:IsLocked="True" Tapped="Spot2Difference3_Tapped" />
        <Image x:Name="Spot2Difference4" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted4.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,200,0,0"  d:IsLocked="True" Tapped="Spot2Difference4_Tapped" />
        <Image x:Name="Spot2Difference5" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted5.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,200,0,0" d:IsLocked="True" Tapped="Spot2Difference5_Tapped"/>
        <Image x:Name="Spot2Difference6" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted6.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="220,200,0,0"  d:IsLocked="True" Tapped="Spot2Difference6_Tapped"/>



        <!--Image block for 3rd image-->
        <Image x:Name="Spot3" Visibility="Visible" Source="/Images/SpotTheDifference3.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,350,0,0"  d:IsLocked="True" Tapped="Spot3_Tapped" />
        <Image x:Name="Spot3Difference" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,350,0,0"  d:IsLocked="True" Tapped="Spot3Difference_Tapped" />
        <Image x:Name="Spot3Difference2" Visibility="Collapsed" Source="/Images/SpotTheDifferenceSpotted2.png" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="20,350,0,0"  d:IsLocked="True" Tapped="Spot3Difference2_Tapped" />
