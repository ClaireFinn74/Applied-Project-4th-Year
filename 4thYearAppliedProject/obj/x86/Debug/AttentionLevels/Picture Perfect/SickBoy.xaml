        <Button x:ConnectionId='6' x:Name="btnTwoHands" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="182,52,0,0" VerticalAlignment="Top"                           IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
