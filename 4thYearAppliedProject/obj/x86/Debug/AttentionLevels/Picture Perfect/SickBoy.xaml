    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock x:ConnectionId='1' x:Name="txtbScore" Margin="1,12,12,0" Opacity="0.5" Text="" Height="52" VerticalAlignment="Top" Grid.Column="3" d:LayoutOverrides="HorizontalAlignment"/>
        <Button x:ConnectionId='2' x:Name="btnBear" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="20,52,0,0" VerticalAlignment="Top"                       IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="One Bear"/>
        <Button x:ConnectionId='3' x:Name="btnStethoscope" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="19,238,0,0" VerticalAlignment="Top"                              IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Stethoscope"/>
        <Button x:ConnectionId='4' x:Name="btnTwoBears" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="27,438,0,0" VerticalAlignment="Top"                           IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Two Bears"/>
        <Button x:ConnectionId='5' x:Name="btnGlasses" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="190,331,0,0" VerticalAlignment="Top"                          IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
