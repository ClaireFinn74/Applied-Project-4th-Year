        <Button x:ConnectionId='7' x:Name="btnTwoPaws" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="190,240,0,0" VerticalAlignment="Top"                          IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
