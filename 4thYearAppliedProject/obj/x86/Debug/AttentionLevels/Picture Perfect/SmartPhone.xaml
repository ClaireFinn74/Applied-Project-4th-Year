<Page
    x:Class="_4thYearAppliedProject.AttentionLevels.Picture_Perect.SmartPhone"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:_4thYearAppliedProject.AttentionLevels.Picture_Perect"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button x:ConnectionId='1' x:Name="btnTenPhones" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="20,52,0,0" VerticalAlignment="Top"                            IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Ten Phones"/>
        <Button x:ConnectionId='2' x:Name="btnElevenPhones" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="19,238,0,0" VerticalAlignment="Top"                               IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Eleven Phones"/>
        <Button x:ConnectionId='3' x:Name="btnBlackberry" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="27,438,0,0" VerticalAlignment="Top"                             IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Blackberry"/>
        <Button x:ConnectionId='4' x:Name="btnTimeTwelve" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="190,331,0,0" VerticalAlignment="Top"                             IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Time 12:00"/>
        <Button x:ConnectionId='5' x:Name="btnButtons" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="182,52,0,0" VerticalAlignment="Top"                          IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Buttons"/>
        <Button x:ConnectionId='6' x:Name="btnAndroidLogo" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="190,240,0,0" VerticalAlignment="Top"                              IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Android Logo"/>
        <Button x:ConnectionId='7' x:Name="btnTimeNine" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="192,143,0,0" VerticalAlignment="Top"                           IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Time 9:00"/>
        <Button x:ConnectionId='8' x:Name="btnS" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="199,435,-2,0" VerticalAlignment="Top"                    IsEnabled="True" Height="74" Width="163" RenderTransformOrigin="0.5,0.5"
         Content="Letter S"/>
        <Button x:ConnectionId='9' x:Name="btnHeadPhones" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="28,336,0,0" VerticalAlignment="Top"                             IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Headphones"/>
        <Button x:ConnectionId='10' x:Name="btnTelephone" Background="Pink" Opacity="1" HorizontalAlignment="Left" Margin="17,144,0,0" VerticalAlignment="Top"                            IsEnabled="True" Height="74" Width="140" RenderTransformOrigin="0.5,0.5"
         Content="Telephone"/>
        <TextBlock x:ConnectionId='11' x:Name="txtbScore" Margin="1,12,12,0" Opacity="0.5" Text="" Height="52" VerticalAlignment="Top" Grid.Column="3" d:LayoutOverrides="HorizontalAlignment"/>
    </Grid>
</Page>

