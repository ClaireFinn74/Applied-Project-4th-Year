<Page
    x:Class="_4thYearAppliedProject.AttentionLevels.Total_Recall.TotalRecallLevel4"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:_4thYearAppliedProject.AttentionLevels.Total_Recall"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Grid x:ConnectionId='1' x:Name="LayoutRoot" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
            <Grid.RowDefinitions>
                <RowDefinition Height="67*"/>
                <RowDefinition Height="13*"/>
            </Grid.RowDefinitions>
            <Image x:ConnectionId='2' x:Name="imgCorrectSign" Visibility="Collapsed" Source="/Images/imgCorrectSign.png" HorizontalAlignment="Left" Width="72" Margin="264,99,0,0" RenderTransformOrigin="1.955,1.778" Height="66" VerticalAlignment="Top"/>
            <Image x:ConnectionId='3' x:Name="imgX" Visibility="Collapsed" Source="/Images/imgX.png" HorizontalAlignment="Left" Width="72" Margin="265,113,0,0" RenderTransformOrigin="1.955,1.778" Height="66" VerticalAlignment="Top"/>
            <TextBox x:ConnectionId='4' x:Name="txtUserInput" FontSize="39" HorizontalAlignment="Left" TextWrapping="Wrap" Text="" VerticalAlignment="Top" RenderTransformOrigin="2.575,3.016" Margin="101,173,-5,0" Width="264" Height="70"/>
            <TextBlock x:ConnectionId='5' x:Name="txtblUserInput" HorizontalAlignment="Left" TextWrapping="Wrap" Text="How many words can you think of?" VerticalAlignment="Top" Height="58" Width="153" Margin="113,341,0,0"/>
            <TextBox x:ConnectionId='6' x:Name="txtScore" HorizontalAlignment="Left" IsReadOnly="True" TextWrapping="Wrap" Text="" VerticalAlignment="Top" RenderTransformOrigin="2.575,3.016" Margin="244,46,0,0" Width="99" Height="47"/>
            <Button x:ConnectionId='7' x:Name="btnEnter" Content="Submit Word" Background="#33EE40BE" BorderBrush="#FF39498F" HorizontalAlignment="Left" Margin="31,277,0,0" VerticalAlignment="Top" Height="58" Width="303"                       />
            <TextBox x:ConnectionId='8' x:Name="txtBrea" HorizontalAlignment="Left" TextWrapping="Wrap" FontSize="39" Text="Brea" VerticalAlignment="Top" Height="70" Width="99" Margin="3,174,0,0" RenderTransformOrigin="0.453,0.9"/>
            <TextBlock x:ConnectionId='9' x:Name="txtTimer" FontSize="25" Margin="110,105,152,382.873"/>
            <TextBlock x:ConnectionId='10' x:Name="txtLog" FontSize="45" Margin="13,109,267,383.873"/>
            <TextBlock x:ConnectionId='11' x:Name="txtbScore" FontSize="39" HorizontalAlignment="Left" Margin="126,41,0,0" TextWrapping="Wrap" Text="Score:" VerticalAlignment="Top"/>
            <Button x:ConnectionId='12' x:Name="btnLevel5" Visibility="Collapsed" Content="Level 5" Background="#33EE40BE" BorderBrush="#FF39498F" HorizontalAlignment="Left" Margin="109,405,0,0" VerticalAlignment="Top" Height="58" Width="147"                        />
        </Grid>
    </Grid>
</Page>

